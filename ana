Inicio
 escreval("Qual é a sua idade?")
  leia(idade)
  
  se (idade<12) entao
  escreval("Você é uma criança")

  fimse
  
  se (idade>=12) e (idade<18) entao
    escreval("Você é um adolescente")
fimse

  se (idade>=18)e (idade<60) entao
   escreval ("Você é adulto")
fimse

  se (idade>=60) entao
  escreval("Você é um velho")
  fimse


